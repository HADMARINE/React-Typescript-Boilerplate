{
  "name": "react-typescript-boilerplate",
  "version": "3.2.1",
  "main": "index.tsx",
  "author": "HADMARINE",
  "license": "MIT",
  "scripts": {
    "dev": "npm run tailwind:css && cross-env react-app-rewired start",
    "build": "npm run tailwind:css && cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "npm run tailwind:css && npx jest --config ./jest.config.js --watch",
    "test-auto": "npm run tailwind:css && npx jest --config ./jest.config.js --forceExit",
    "eject": "react-scripts eject",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "enc:env": "openssl aes-256-cbc -e -in .env -out .env.pub -md md5 -k $PROGRAM_ENV_SECRET_KEY",
    "dec:env": "openssl aes-256-cbc -d -in .env.pub -out .env -md md5 -k $PROGRAM_ENV_SECRET_KEY",
    "dec:env-m": "openssl aes-256-cbc -d -in .env.pub -out .env -md md5 -k",
    "tailwind:css": "tailwind build src/styles/tailwind.src.css -c tailwind.config.js -o src/styles/tailwind.min.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eqb-request-client": "0.7.0-rc3",
    "immutable": "4.0.0-rc.14",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "mobx-state-tree": "^5.0.2",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "require-strip-json-comments": "^2.0.0",
    "styled-components": "^5.3.0",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.2",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "core-decorators": "^0.20.0",
    "customize-cra": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-jest": "^24.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.6.0",
    "prettier": "^2.3.2",
    "react-app-rewire-alias": "^1.0.3",
    "react-app-rewired": "^2.1.8",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.5"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}