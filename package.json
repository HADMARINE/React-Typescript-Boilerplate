{
  "name": "react-typescript-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run tailwind:css && cross-env react-app-rewired start",
    "build": "npm run tailwind:css && cross-env react-app-rewired build",
    "test": "npm run tailwind:css && react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "tailwind:css": "tailwind build src/styles/tailwind.src.css -c tailwind.config.js -o src/styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.0.0",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.1",
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "immutable": "^4.0.0-rc.12",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "redux": "^4.0.5",
    "styled-components": "^4.4.1",
    "tailwindcss": "^1.1.4",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "customize-cra": "^0.9.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^23.7.0",
    "prettier": "^1.19.1",
    "react-app-rewire-alias": "^0.1.3",
    "react-app-rewired": "^2.1.5"
  }
}
